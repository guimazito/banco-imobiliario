        },
        "icon": {
          "type": "string",
          "example": "ACCOUNT_CIRCLE"
        },
        "createdAt": {
          "type": "string",
          "example": "2025-09-16T19:27:15.645Z"
        },
        "updatedAt": {
          "type": "string",
          "example": "2025-09-16T19:27:15.645Z"
        },
        "deletedAt": {}
      }
    },
    "CreatePlayerDto": {
